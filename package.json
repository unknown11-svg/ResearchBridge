{
  "name": "researchbridge",
  "version": "1.0.0",
  "description": "University Research Collaboration Platform",
  "private": true,
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
