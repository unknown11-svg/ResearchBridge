  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unknown11-svg/ResearchBridge/issues"
  },
  "homepage": "https://github.com/unknown11-svg/ResearchBridge#readme",
  "dependencies": {
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "passport": "^0.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "^5.0.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
